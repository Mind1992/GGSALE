<%= form_for sale do |f| %>
	<div class="col-sm-6 col-md-3">
		<div class="thumbnail">
      <h3 class="text-center"><%= sale.title %></h3>
      <p class="text-center"><%= sale.format_date %></p>
      <p class="text-center"><%= sale.address %></p>
      <p class="text-center"><%= link_to "View", sale_path(sale), class: "btn btn-primary", role: "button" %></p>
		</div>
	</div>
<% end %>